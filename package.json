  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sws2apps/cpe-sws.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && workbox generateSW workbox.config.cjs",
