    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "rollup": "^4.12.0",
    "serve": "^14.2.1",
    "typescript": "^5.3.3",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">18 <21",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json,html}": "prettier --write"
  }
}
