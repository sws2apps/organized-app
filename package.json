{
  "name": "organized",
  "private": true,
  "version": "3.0.0",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/sws2apps/organized-app.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && workbox generateSW workbox.config.cjs",
    "preview": "serve -l 5000 -s dist",
    "test": "npm run cypress:component-test",
    "cypress:open": "cypress open",
    "cypress:component-test": "cypress run --component --browser chrome",
    "generate:css": "node converter/css/convert.js && prettier src/v3/global.css --write",
    "generate:icons": "node converter/svg/convert.js && prettier src/v3/components/icons --write",
    "prepare": "husky install",
    "locales:unused": "node unused-i18n-keys.js",
    "lint": "eslint src/v3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fingerprintjs/fingerprintjs": "^4.2.2",
    "@fontsource/inter": "^5.0.17",
    "@fontsource/poppins": "^5.0.12",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.12",
    "@mui/x-date-pickers": "^6.19.6",
    "@mui/x-tree-view": "^6.17.0",
    "@react-pdf/renderer": "^3.3.8",
    "@sws2apps/react-sw-helper": "^1.21.0",
    "@tanstack/react-query": "^5.25.0",
    "comlink": "^4.4.1",
    "crypto-es": "^2.1.0",
    "date-fns": "^2.30.0",
    "dateformat": "^5.0.3",
    "dexie": "^3.2.6",
    "dexie-react-hooks": "^1.1.7",
    "eslint-config-prettier": "^9.1.0",
    "fast-memoize": "^2.5.2",
    "file-select-dialog": "^1.5.4",
    "firebase": "^10.8.1",
    "html2pdf.js": "^0.10.1",
    "i18next": "^23.10.0",
    "interweave": "^13.1.0",
    "jw-epub-parser": "^3.12.1",
    "mui-one-time-password-input": "^2.0.2",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-pdf-html": "^1.2.0",
    "react-router-dom": "^6.22.3",
    "recoil": "^0.7.7",
    "recoil-outside": "^0.1.1",
    "swiper": "^11.0.7",
    "use-pwa2": "^0.2.0",
    "validator": "^13.11.0",
    "vite": "^5.1.5",
    "vite-plugin-comlink": "^4.0.2",
    "workbox-cli": "^7.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgx/vite-plugin-react": "^1.0.1",
    "@sws2apps/vite-plugin-package-version": "^1.4.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "rollup": "^4.12.1",
    "serve": "^14.2.1",
    "typescript": "^5.4.2",
    "vite-plugin-eslint": "^1.8.1"
  },
  "engines": {
    "node": ">18 <21",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json,html}": "prettier --write"
  }
}
